    .headroom--pinned {
        display: block;
    }

    .headroom--unpinned {
        display: none;
    }

    .upload-thumbnail:hover {

        transform: scale(1.5) translate(0px, 30px);

        -webkit-filter: brightness(120%);
    }

    .upload-thumbnail {
        transition: transform .7s ease;
    }

    .holder {
        overflow: hidden; /* [1.2] Hide the overflowing of child elements */
    }

